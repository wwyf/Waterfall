@startuml
''' 省略底部
	hide footbox 

''' 定义类
	actor User as act
	boundary LoginInterface as bou
	control LoginController as con
	database UserList as db

	autonumber
	act -> bou : fillLoginInfo()
	bou -> con : isLoginInfoValid()
	con -> db : Query()
	db -> con : ReturnTrue
	con -> bou : LoginResult()
	bou -> bou : Display()

@enduml